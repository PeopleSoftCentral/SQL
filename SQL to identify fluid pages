SELECT PNLNAME "Page Name",
CASE WHEN A.PNLTYPE = 0 THEN 'Standard'
WHEN A.PNLTYPE = 1 THEN 'SubPage'
WHEN A.PNLTYPE = 2 THEN 'Secondary'
WHEN A.PNLTYPE = 3 THEN 'Popup Page'
WHEN A.PNLTYPE = 4 THEN 'Header Page'
WHEN A.PNLTYPE = 5 THEN 'Side Page 1'
WHEN A.PNLTYPE = 6 THEN 'Footer Page'
WHEN A.PNLTYPE = 7 THEN 'Layout Page'
WHEN A.PNLTYPE = 8 THEN 'Search Page'
WHEN A.PNLTYPE = 9 THEN 'Prompt Page'
WHEN A.PNLTYPE = 10 THEN 'Master and Detail Target Page'
WHEN A.PNLTYPE = 11 THEN 'Side Page 2'
ELSE TO_CHAR( A.PNLTYPE) END "Page Type"
FROM PSPNLDEFN a WHERE BITAND(PNLUSE, 16384) = 16384;
